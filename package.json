{
  "name": "crewbot-discord",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/CrewBot-dev/crewbot-discord",
  "author": "Haleon, Ray Redondo, Isaac Kilbourne",
  "license": "BSD+Patent",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts | pino-pretty --translateTime \"HH:MM:ss\" --ignore pid,hostname",
    "build": "tsc --outDir ./build",
    "start": "node ./build/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "json5": "^2.2.1",
    "pino": "^8.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.3",
    "pino-pretty": "^9.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "prettier": {
    "tabWidth": 2,
    "endOfLine": "lf",
    "arrowParens": "always",
    "singleQuote": true,
    "printWidth": 100
  }
}
